<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.onlineacademy.model.CourseContentModel">
        <table name="course_content" schema="onlineAcademy" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="courseId">
                <column name="courseId" precision="-1"/>
            </basic>
            <basic name="isPreview">
                <column name="isPreview" precision="1"/>
            </basic>
            <basic name="content">
                <column name="content" nullable="false"/>
            </basic>
            <basic name="video">
                <column name="video" nullable="false"/>
            </basic>
            <basic name="isDeleted">
                <column name="isDeleted" precision="1"/>
            </basic>
            <basic name="lastUpdated">
                <column name="lastUpdated" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
